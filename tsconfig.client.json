      "outDir": "./public/js/",
      "noImplicitAny": true,
      "module": "es6",
      "typeRoots": [
          "node_modules/@types"
      ],
      "target": "es5",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "allowJs": true,
      "lib":[
          "es2015", "es2019.Object","dom", 
      ]
    },
    "include": [
        "src/client/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
  }